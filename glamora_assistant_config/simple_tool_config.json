  "method": "POST",
  "parameters": {
    "customerName": {
      "type": "string",
      "required": true,
      "description": "Customer's full name"
    },
    "service": {
      "type": "string",
      "required": true,
      "description": "Service name (strihanie, farbenie, mel√≠r, etc.)"
    },
    "staffName": {
      "type": "string",
      "required": true,
