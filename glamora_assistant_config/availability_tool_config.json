          "constant_value": "",
          "required": true
        },
        {
          "id": "service",
          "type": "string",
          "value_type": "llm_prompt",
          "description": "Služba: strihanie, farbenie, melír, balayage",
          "dynamic_variable": "",
          "constant_value": "",
          "required": true
        }
      ],
      "required": false,
      "value_type": "llm_prompt"
    },
    "request_headers": [
      {
        "type": "value",
        "name": "Content-Type",
        "value": "application/json"
      }
