  "disable_interruptions": false,
  "force_pre_tool_speech": "auto",
  "assignments": [],
  "api_schema": {
    "url": "https://teamupkalendar-production.up.railway.app/api/webhook/availability",
    "method": "POST",
    "path_params_schema": [],
    "query_params_schema": [],
