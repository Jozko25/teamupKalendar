{
  "type": "webhook",
  "name": "teamup-booking-manager",
  "description": "VŽDY použi tento nástroj pri vytváraní rezervácie - POVINNÉ!",
  "disable_interruptions": false,
  "force_pre_tool_speech": "auto",
  "api_schema": {
    "url": "https://api.teamup.com/ks65ktirgttv3hdobb/events",
    "method": "POST",
    "headers": {
      "Teamup-Token": "414e212850efa563e91ca0d2dfcef7e449d6eeb6f1c1380e10370aedf0b407be",
      "Content-Type": "application/json"
    },
    "request_body_schema": {
      "type": "object",
      "properties": [
        {
          "id": "title",
          "type": "string",
          "value_type": "llm_prompt",
          "description": "Názov rezervácie vo formáte: [Meno klienta] - [Služba]",
          "required": true
        },
        {
          "id": "subcalendar_id",
          "type": "integer",
          "value_type": "llm_prompt",
          "description": "ID subcalendar: Janka=14791751, Nika=14791752 (Dominika a Lívia nemajú ešte vytvorené)",
          "required": true
        },
        {
          "id": "start_dt",
          "type": "string",
          "value_type": "llm_prompt",
          "description": "Začiatok vo formáte ISO 8601 (napr. 2025-09-19T14:00:00)",
          "required": true
        },
        {
          "id": "end_dt",
          "type": "string",
          "value_type": "llm_prompt",
          "description": "Koniec vo formáte ISO 8601 - vypočítaj podľa trvania služby",
          "required": true
        },
        {
          "id": "who",
          "type": "string",
          "value_type": "llm_prompt",
          "description": "Meno zákazníka",
          "required": true
        },
        {
          "id": "notes",
          "type": "string",
          "value_type": "llm_prompt",
          "description": "Poznámky - uveď službu a meno personálu",
          "required": false
        }
      ]
    }
  },
  "response_handling": {
    "success_path": "event.id",
    "error_path": "error.message"
  }
}