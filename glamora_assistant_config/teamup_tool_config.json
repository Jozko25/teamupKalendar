{
  "type": "webhook",
  "name": "teamup-booking-manager",
  "description": "Nástroj na vytvorenie rezervácie v kalendári Glamora Studio - POVINNÉ ZAVOLAŤ PRI KAŽDEJ REZERVÁCII",
  "disable_interruptions": false,
  "force_pre_tool_speech": "auto",
  "assignments": [],
  "api_schema": {
    "url": "https://teamupkalendar-production.up.railway.app/api/bookings",
    "method": "POST",
    "path_params_schema": [],
    "query_params_schema": [],
    "request_body_schema": {
      "id": "body",
      "type": "object",
      "description": "Vytvorenie novej rezervácie",
      "properties": [
        {
          "id": "title",
          "type": "string",
          "value_type": "llm_prompt",
          "description": "Format: '[Meno klienta] - [Meno personálu] - [Služba]' (napr. 'Jana Nováková - Janka - Strih')",
          "dynamic_variable": "",
          "constant_value": "",
          "required": true
        },
        {
          "id": "subcalendarId",
          "type": "string",
          "value_type": "llm_prompt",
          "description": "ID kalendára - VŽDY použiť '14750194' (jednotný kalendár)",
          "dynamic_variable": "",
          "constant_value": "",
          "required": true
        },
        {
          "id": "startTime",
          "type": "string",
          "value_type": "llm_prompt",
          "description": "Začiatok termínu vo formáte ISO 8601 (napr. 2025-01-15T14:00:00)",
          "dynamic_variable": "",
          "constant_value": "",
          "required": true
        },
        {
          "id": "duration",
          "type": "integer",
          "value_type": "llm_prompt",
          "description": "Trvanie v minútach (predvolene 60)",
          "dynamic_variable": "",
          "constant_value": "",
          "required": false
        },
        {
          "id": "location",
          "type": "string",
          "value_type": "llm_prompt",
          "description": "Miesto rezervácie - vždy 'Glamora Studio'",
          "dynamic_variable": "",
          "constant_value": "",
          "required": false
        },
        {
          "id": "customerInfo",
          "type": "object",
          "description": "Kontaktné údaje zákazníka",
          "properties": [
            {
              "id": "name",
              "type": "string",
              "value_type": "llm_prompt",
              "description": "Meno a priezvisko zákazníka",
              "dynamic_variable": "",
              "constant_value": "",
              "required": false
            },
            {
              "id": "phone",
              "type": "string",
              "value_type": "llm_prompt",
              "description": "Telefónne číslo zákazníka",
              "dynamic_variable": "",
              "constant_value": "",
              "required": false
            },
            {
              "id": "email",
              "type": "string",
              "value_type": "llm_prompt",
              "description": "Email zákazníka (nepovinné)",
              "dynamic_variable": "",
              "constant_value": "",
              "required": false
            }
          ],
          "required": false,
          "value_type": "llm_prompt"
        },
        {
          "id": "notes",
          "type": "string",
          "value_type": "llm_prompt",
          "description": "Názov služby v slovenčine a meno personálu (napr. 'Strih - Janka')",
          "dynamic_variable": "",
          "constant_value": "",
          "required": false
        }
      ],
      "required": false,
      "value_type": "llm_prompt"
    },
    "request_headers": [
      {
        "type": "value",
        "name": "Content-Type",
        "value": "application/json"
      }
    ],
    "auth_connection": null
  },
  "response_timeout_secs": 20,
  "dynamic_variables": {
    "dynamic_variable_placeholders": {}
  }
}