{
  "type": "webhook",
  "name": "glamora-booking-system",
  "description": "Kompletný systém pre kontrolu dostupnosti a vytvorenie rezervácií",
  "disable_interruptions": false,
  "force_pre_tool_speech": "auto",
  "assignments": [],
  "api_schema": {
    "url": "https://teamupkalendar-production.up.railway.app/api/webhook",
    "method": "POST",
    "path_params_schema": [],
    "query_params_schema": [],
    "request_body_schema": {
      "id": "body",
      "type": "object",
      "description": "Booking system request",
      "properties": [
        {
          "id": "action",
          "type": "string",
          "value_type": "llm_prompt",
          "description": "Akcia: 'availability' pre kontrolu dostupnosti alebo 'booking' pre vytvorenie rezervácie",
          "dynamic_variable": "",
          "constant_value": "",
          "required": true
        },
        {
          "id": "staffName",
          "type": "string",
          "value_type": "llm_prompt",
          "description": "Meno personálu: Janka alebo Nika",
          "dynamic_variable": "",
          "constant_value": "",
          "required": true
        },
        {
          "id": "date",
          "type": "string",
          "value_type": "llm_prompt",
          "description": "Dátum vo formáte YYYY-MM-DD (VŽDY rok 2025!, napr. 2025-09-25)",
          "dynamic_variable": "",
          "constant_value": "",
          "required": true
        },
        {
          "id": "service",
          "type": "string",
          "value_type": "llm_prompt",
          "description": "Služba: strihanie, farbenie, melír, balayage",
          "dynamic_variable": "",
          "constant_value": "",
          "required": true
        },
        {
          "id": "customerName",
          "type": "string",
          "value_type": "llm_prompt",
          "description": "Meno a priezvisko zákazníka (len pri action='booking')",
          "dynamic_variable": "",
          "constant_value": "",
          "required": false
        },
        {
          "id": "time",
          "type": "string",
          "value_type": "llm_prompt",
          "description": "Čas vo formáte HH:MM (len pri action='booking', napr. 14:00)",
          "dynamic_variable": "",
          "constant_value": "",
          "required": false
        }
      ],
      "required": false,
      "value_type": "llm_prompt"
    },
    "request_headers": [
      {
        "type": "value",
        "name": "Content-Type",
        "value": "application/json"
      }
    ],
    "auth_connection": null
  },
  "response_timeout_secs": 20,
  "dynamic_variables": {
    "dynamic_variable_placeholders": {}
  }
}